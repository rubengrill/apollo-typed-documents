{
  "name": "apollo-typed-documents",
  "version": "0.0.3",
  "description": "Get type safety for your apollo documents.",
  "keywords": [
    "apollo",
    "graphql",
    "codegen"
  ],
  "homepage": "https://github.com/rubengrill/apollo-typed-documents",
  "repository": {
    "url": "https://github.com/rubengrill/apollo-typed-documents",
    "type": "git"
  },
  "author": "Ruben Grill <ruben@purelabs.io>",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf lib && tsc",
    "preversion": "yarn test",
    "version": "yarn run build",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "lint": "eslint --ext .js,.ts src/ examples/docs/",
    "docs": "yarn run build && (cd examples/docs && graphql-codegen --config codegenApolloMock.yml && graphql-codegen --config codegenTypedDocuments.yml) && md-magic --path README.md"
  },
  "peerDependencies": {
    "graphql": "^14.6.0",
    "pascal-case": "^3.1.1"
  },
  "devDependencies": {
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^3.1.4",
    "@graphql-codegen/cli": "^1.13.3",
    "@graphql-codegen/core": "^1.13.2",
    "@graphql-codegen/plugin-helpers": "^1.13.2",
    "@graphql-codegen/typescript": "^1.13.3",
    "@graphql-codegen/typescript-operations": "^1.13.3",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/tmp": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "apollo-boost": "^0.4.7",
    "apollo-typed-documents": "link:.",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "graphql": "^14.6.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^25.3.0",
    "jest-transform-graphql": "^2.1.0",
    "markdown-magic": "^1.0.0",
    "pascal-case": "^3.1.1",
    "prettier": "^2.0.4",
    "prettier-plugin-import-sort": "^0.0.4",
    "react": "^16.13.1",
    "rimraf": "^3.0.2",
    "tmp": "^0.1.0",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "importSort": {
    ".js,.ts": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
